{
    "name": "dashboard",
    "displayName": "dashboard",
    "description": "",
    "version": "0.1.1",
    "publisher": "Kruemelkatze",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/dashboard",
    "contributes": {
        "commands": [
            {
                "command": "dashboard.open",
                "title": "Dashboard: Open"
            },
            {
                "command": "dashboard.addProject",
                "title": "Dashboard: Add Project"
            },
            {
                "command": "dashboard.removeProject",
                "title": "Dashboard: Remove Project"
            },
            {
                "command": "dashboard.editProjects",
                "title": "Dashboard: Edit Projects"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.9.5",
        "gulp": "^4.0.0",
        "gulp-sass": "^4.0.1",
        "node-sass": "^4.9.3",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "mkdirp": "^0.5.1"
    }
}
